        <TextBlock x:Name="textBlock1_Copy1" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Margin="95,51,0,0" Foreground="White" Width="108" Text="By Claim Reference"/>
        <Button x:Name="button" Content="Cancel" HorizontalAlignment="Left" Height="25" Margin="49,265,0,0" VerticalAlignment="Top" Width="79" Click="button_Click"/>
