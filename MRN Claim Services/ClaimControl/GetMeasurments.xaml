    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:MRN_Claim_Services.Controllers"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="300"
