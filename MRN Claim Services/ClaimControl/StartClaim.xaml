<Page x:Class="MRN_Claim_Services.Controllers.StartClaim"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MRN_Claim_Services.Controllers"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="StartClaim">

    <Grid>
        <Button x:Name="Startbutton" Content="Start" HorizontalAlignment="Left" Margin="215,270,0,0" VerticalAlignment="Top" Width="75" Click="Startbutton_Click"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Center" Margin="36,10,44,0" TextWrapping="Wrap" Text="Claim Starts Here" VerticalAlignment="Top" Height="36" Width="220" Foreground="White" FontSize="24" FontWeight="Bold" TextDecorations="Underline" TextAlignment="Center"/>

    </Grid>
</Page>
