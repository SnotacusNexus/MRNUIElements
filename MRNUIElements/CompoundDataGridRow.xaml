<UserControl x:Class="MRNUIElements.CompoundDataGridRow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:Models="clr-namespace:MRNUIElements.Models"
             xmlns:ViewModels="clr-MRNUIElements.ViewModels"
             xmlns:calc="http://clr-namespace:MRNUIElements.CompoundDataGridRow"
             xmlns:chart="clr-namespace:Syncfusion.UI.Xaml.Charts;assembly=Syncfusion.SfChart.WPF"
             xmlns:converters="clr-namespace:MRNUIElements.ViewModels.Converters"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:layout="clr-namespace:Syncfusion.Windows.SampleLayout;assembly=Syncfusion.Chart.Wpf.SampleLayout"
             xmlns:local="clr-namespace:MRNUIElements"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
             x:Name="DetailedClaimGridRow"
             BorderBrush="{DynamicResource ReverseBackgroundGradient}"
             BorderThickness="2"
             Loaded="DetailedClaimGridRow_Loaded"
             mc:Ignorable="d">
    <UserControl.Resources>
        <converters:NumberSign x:Key="NumberSign" />
        <converters:BackgroundConverter x:Key="BackGroundConverter" />
        <ImageBrush x:Key="MRNLOGO" ImageSource=".\ResourceFiles\MRN CONTRACTING LOGO.png" />
        <CollectionViewSource x:Key="scopeModelViewSource" d:DesignSource="{d:DesignInstance {x:Type Models:ScopeModel}, CreateList=True}" />
    </UserControl.Resources>
    <UserControl.DataContext>
        <Models:ScopeModel />
    </UserControl.DataContext>






    <Grid x:Name="layoutRoot"
          Width="1200"
          Height="175">
        <Grid.Resources />
        <Grid.Background>
            <Binding Converter="{StaticResource BackGroundConverter}"
                     ConverterParameter="value"
                     ElementName="ProfitabilityValue"
                     NotifyOnSourceUpdated="True"
                     NotifyOnTargetUpdated="True"
                     Path="Value"
                     StringFormat="{}{0:F}" />


        </Grid.Background>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100*" />
            <ColumnDefinition Width="100*" />
            <ColumnDefinition Width="15*" />
            <ColumnDefinition Width="100*" />
            <ColumnDefinition Width="60*" />
            <ColumnDefinition Width="60*" />
            <ColumnDefinition Width="60*" />
            <ColumnDefinition Width="60*" />
            <ColumnDefinition Width="60*" />
            <ColumnDefinition Width="60*" />
            <ColumnDefinition Width="60*" />
            <ColumnDefinition Width="60*" />
            <ColumnDefinition Width="60*" />
            <ColumnDefinition Width="60*" />
            <ColumnDefinition Width="60*" />
            <ColumnDefinition Width="60*" />


        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="50" />
            <RowDefinition Height="50*" />
            <RowDefinition Height="50" />
            <RowDefinition Height="25" />
        </Grid.RowDefinitions>
        <TextBlock x:Name="ClaimNumber"
                   Height="25"
                   HorizontalAlignment="Stretch"
                   VerticalAlignment="Bottom"
                   Foreground="Black"
                   TextWrapping="Wrap" />
        <syncfusion:IntegerTextBox x:Name="ClaimAge"
                                   Height="25"
                                   HorizontalAlignment="Stretch"
                                   VerticalAlignment="Top"
                                   HorizontalContentAlignment="Center"
                                   Background="Transparent"
                                   Foreground="Black"
                                   IsReadOnly="True"
                                   PositiveForeground="#FF040404"
                                   Text="IntegerTextBox"
                                   TextWrapping="NoWrap"
                                   ZeroColor="White"
                                   Value="0" />
        <Grid>

            <Grid.DataContext>
                <Models:ScopeModel />
            </Grid.DataContext>

            <Grid.Resources>
                <local:ColorConverter x:Key="colorConv" />
                <local:Labelconvertor x:Key="labelconverter" />

                <!--  Adornment Label Template  -->
                <DataTemplate x:Key="labelTemplate1">
                    <Grid>
                        <Rectangle Fill="{Binding Converter={StaticResource colorConv}}"
                                   Stroke="White"
                                   StrokeThickness="1" />
                        <TextBlock Margin="6,3,6,3"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Top"
                                   FontSize="12"
                                   Foreground="White"
                                   Text="{Binding Converter={StaticResource labelconverter}}" />
                    </Grid>
                </DataTemplate>

                <!--  ToolTip Template  -->
                <DataTemplate x:Key="TooltipTemplate">
                    <Grid IsHitTestVisible="False">
                        <Rectangle Fill="{Binding Interior}"
                                   Stroke="White"
                                   StrokeThickness="1" />
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Margin="5,3,5,3"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Top"
                                       FontSize="11"
                                       Foreground="White"
                                       IsHitTestVisible="False"
                                       Text="{Binding Item.Expense}" />
                            <TextBlock Margin="5,3,5,3"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Top"
                                       FontSize="11"
                                       Foreground="White"
                                       Text=":" />
                            <TextBlock Margin="5,3,5,3"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Top"
                                       FontSize="11"
                                       Foreground="White"
                                       IsHitTestVisible="False"
                                       Text="{Binding YData}" />
                        </StackPanel>
                    </Grid>
                </DataTemplate>

                <!--  LegendItem Template  -->
                <DataTemplate x:Key="legend">
                    <StackPanel Orientation="Horizontal">
                        <Grid Margin="30,0,0,0">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>

                            <Rectangle Width="15"
                                       Height="15"
                                       Fill="{Binding Interior}" />
                            <TextBlock Grid.Column="1"
                                       Margin="10,5,5,0"
                                       FontSize="16"
                                       Foreground="Black"
                                       Text="{Binding Item.Expense}" />
                        </Grid>
                    </StackPanel>
                </DataTemplate>
            </Grid.Resources>
        </Grid>
