                  HorizontalAlignment="Left"
                  VerticalAlignment="Top">
                <Grid.RowDefinitions>
                    <RowDefinition Height="335*" />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <syncfusion:MaskedTextBox x:Name="Cust_Suffix_Copy1"
