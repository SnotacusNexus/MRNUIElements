                Height="1879"
                Margin="10,10,0,-34"
                HorizontalAlignment="Left"
                VerticalAlignment="Top" />
        <Image x:Name="image"
               Width="90"
               Height="100"
               Margin="1315,55,0,0"
               HorizontalAlignment="Left"
               VerticalAlignment="Top" />
        <Grid Grid.ColumnSpan="2"
              Width="Auto"
              Height="Auto"
              Margin="40,-27,0,23">
