<Window x:Class="MRNUIElements.Rake_Length"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MRNUIElements"
        mc:Ignorable="d"
        Title="Rake_Length" Height="243.706" Width="390.236" Background="{x:Null}" ResizeMode="NoResize" WindowStyle="ToolWindow">
	<Grid Margin="0,0,2,1">
		<Button x:Name="button" Content="_OK" HorizontalAlignment="Right" Margin="0,114,0,0" VerticalAlignment="Top" Width="76" IsDefault="True" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White" Height="23"/>
		<Button x:Name="button1" Content="_Cancel" HorizontalAlignment="Right" Margin="0,142,0,0" VerticalAlignment="Top" Width="76" IsCancel="True" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White" Height="24" RenderTransformOrigin="0.447,0.542"/>
