                    TextWrapping="Wrap" Background="{x:Null}" Foreground="White" WatermarkTextForeground="White" WatermarkBackground="{x:Null}" WatermarkText="Enter Zipcode --&gt;" Margin="66,160,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" HorizontalContentAlignment="Center" Width="125" />
                    <syncfusion:MaskedTextBox x:Name="Lead_State"
                    Height="22"
                    BorderThickness="1"
