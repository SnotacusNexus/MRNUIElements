                    WatermarkText="Zipcode" Foreground="White" WatermarkTextForeground="White" Margin="317,160,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" HorizontalContentAlignment="Center" />
                    <syncfusion:MaskedTextBox x:Name="Lead_Primary_Phone"
                    Height="22"
                    MinWidth="100"
