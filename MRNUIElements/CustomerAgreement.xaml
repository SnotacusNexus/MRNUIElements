                    TextWrapping="Wrap" Background="{x:Null}" Foreground="White" WatermarkTextForeground="White" WatermarkBackground="{x:Null}" WatermarkText="Enter Zipcode --&gt;" Margin="196,160,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" HorizontalContentAlignment="Center" Width="116"  />
                    <syncfusion:MaskedTextBox x:Name="Lead_Zipcode"
                    Height="22"
                    Background="{x:Null}"
                    Text="{Binding Zip}"
