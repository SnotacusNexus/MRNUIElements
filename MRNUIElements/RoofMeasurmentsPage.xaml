<Page x:Class="MRNUIElements.RoofMeasurmentsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:Custom="http://schemas.awesomium.com/winfx"
      xmlns:MRNNexus_Model="clr-namespace:MRNNexus_Model;assembly=MRNNexus_Model"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:MRNUIElements"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
      Title="RoofMeasurmentsPage"
      Width="1005"
      Height="1071.5"
      mc:Ignorable="d">

    <Page.Resources>
        <CollectionViewSource x:Key="dTO_PlaneViewSource" d:DesignSource="{d:DesignInstance {x:Type MRNNexus_Model:DTO_Plane}, CreateList=True}" />
        <Style TargetType="{x:Type ComboBox}">
            <Setter Property="FocusVisualStyle" Value="{StaticResource FocusVisual}" />
            <Setter Property="Background" Value="{StaticResource ComboBox.Static.Background}" />
            <Setter Property="BorderBrush" Value="{StaticResource ComboBox.Static.Border}" />
            <Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.WindowTextBrushKey}}" />
            <Setter Property="BorderThickness" Value="1" />
            <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Auto" />
            <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto" />
            <Setter Property="Padding" Value="6,3,5,3" />
            <Setter Property="ScrollViewer.CanContentScroll" Value="true" />
            <Setter Property="ScrollViewer.PanningMode" Value="Both" />
            <Setter Property="Stylus.IsFlicksEnabled" Value="False" />
            <Setter Property="Template" Value="{StaticResource ComboBoxTemplate}" />
            <Style.Triggers>
                <Trigger Property="IsEditable" Value="true">
                    <Setter Property="IsTabStop" Value="false" />
                    <Setter Property="Padding" Value="2" />
                    <Setter Property="Template" Value="{StaticResource ComboBoxEditableTemplate}" />
                </Trigger>
            </Style.Triggers>
        </Style>
    </Page.Resources>

    <Grid x:Name="Maingrid"
          Width="1000"
          Margin="2,0,3,54">
        <Grid.RowDefinitions>
            <RowDefinition />

        </Grid.RowDefinitions>
        <DockPanel x:Name="Dock"
                   Margin="0,0,0,245"
                   Background="Transparent">
            <StackPanel Height="100"
                        Background="Transparent"
                        DockPanel.Dock="Top">

                <TextBlock Height=" 100 "
                           Background="Transparent"
                           FontSize="60"
                           Foreground="White"
                           Text="Roof Measurements/ Order"
                           TextAlignment="Center" />
            </StackPanel>
            <Grid x:Name="grid1"
                  Height="708"
                  Margin="0"
                  HorizontalAlignment="Left"
                  VerticalAlignment="Top"
                  DataContext="{StaticResource dTO_PlaneViewSource}"
                  DockPanel.Dock="Left">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />

                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>

                <Label Grid.Row="0"
                       Grid.Column="0"
                       Margin="3"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       Content="Eave Height:"
                       Foreground="White" />
                <TextBox x:Name="eaveHeightTextBox"
                         Grid.Row="0"
                         Grid.Column="1"
                         Width="120"
                         Height="23"
                         Margin="3"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Center"
                         Background="Transparent"
                         Text="{Binding EaveHeight,
                                        Mode=TwoWay,
                                        NotifyOnValidationError=true,
                                        ValidatesOnExceptions=true}" />
                <Label Grid.Row="1"
                       Grid.Column="0"
                       Margin="3"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       Content="Eave Length:"
                       Foreground="White" />
                <TextBox x:Name="eaveLengthTextBox"
                         Grid.Row="1"
                         Grid.Column="1"
                         Width="120"
                         Height="23"
                         Margin="3"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Center"
                         Background="Transparent"
                         Text="{Binding EaveLength,
                                        Mode=TwoWay,
                                        NotifyOnValidationError=true,
                                        ValidatesOnExceptions=true}" />
                <Label Grid.Row="2"
                       Grid.Column="0"
                       Margin="3"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       Content="Exception:"
                       Foreground="White"
                       Visibility="Collapsed" />
                <TextBox x:Name="exceptionTextBox"
                         Grid.Row="2"
                         Grid.Column="1"
                         Width="120"
                         Height="23"
                         Margin="3"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Center"
                         Text="{Binding Exception,
                                        Mode=TwoWay,
                                        NotifyOnValidationError=true,
                                        ValidatesOnExceptions=true}"
                         Visibility="Collapsed" />
                <Label Grid.Row="3"
                       Grid.Column="0"
                       Margin="3"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       Content="Four And Up:"
                       Foreground="White" />
                <TextBox x:Name="fourAndUpTextBox"
                         Grid.Row="3"
                         Grid.Column="1"
                         Width="120"
                         Height="23"
                         Margin="3"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Center"
                         Background="Transparent"
                         Text="{Binding FourAndUp,
                                        Mode=TwoWay,
                                        NotifyOnValidationError=true,
                                        ValidatesOnExceptions=true}" />
                <Label Grid.Row="4"
                       Grid.Column="0"
                       Margin="3"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       Content="Group Number:"
                       Foreground="White" />
                <TextBox x:Name="groupNumberTextBox"
                         Grid.Row="4"
                         Grid.Column="1"
                         Width="120"
                         Height="23"
                         Margin="3"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Center"
                         Background="Transparent"
                         Text="{Binding GroupNumber,
                                        Mode=TwoWay,
                                        NotifyOnValidationError=true,
                                        ValidatesOnExceptions=true}" />
                <Label Grid.Row="5"
                       Grid.Column="0"
                       Margin="3"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       Content="Hip Valley:"
                       Foreground="White" />
                <TextBox x:Name="hipValleyTextBox"
                         Grid.Row="5"
                         Grid.Column="1"
                         Width="120"
                         Height="23"
                         Margin="3"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Center"
                         Background="Transparent"
                         Text="{Binding HipValley,
                                        Mode=TwoWay,
                                        NotifyOnValidationError=true,
                                        ValidatesOnExceptions=true}" />
                <Label Grid.Row="6"
                       Grid.Column="0"
                       Margin="3"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       Content="Inner Exception:"
                       Foreground="White"
                       Visibility="Collapsed" />
                <TextBox x:Name="innerExceptionTextBox"
                         Grid.Row="6"
                         Grid.Column="1"
                         Width="120"
                         Height="23"
                         Margin="3"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Center"
                         Text="{Binding InnerException,
                                        Mode=TwoWay,
                                        NotifyOnValidationError=true,
                                        ValidatesOnExceptions=true}"
                         Visibility="Collapsed" />
                <Label Grid.Row="7"
                       Grid.Column="0"
                       Margin="3"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       Content="Inspection ID:"
                       Foreground="White" />
                <TextBox x:Name="inspectionIDTextBox"
                         Grid.Row="7"
                         Grid.Column="1"
                         Width="120"
                         Height="23"
                         Margin="3"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Center"
                         Background="Transparent"
                         Text="{Binding InspectionID,
                                        Mode=TwoWay,
                                        NotifyOnValidationError=true,
                                        ValidatesOnExceptions=true}" />
                <Label Grid.Row="8"
                       Grid.Column="0"
                       Margin="3"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       Content="Item Spec:"
                       Foreground="White" />
                <TextBox x:Name="itemSpecTextBox"
                         Grid.Row="8"
                         Grid.Column="1"
                         Width="120"
                         Height="23"
                         Margin="3"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Center"
                         Background="Transparent"
                         Text="{Binding ItemSpec,
                                        Mode=TwoWay,
                                        NotifyOnValidationError=true,
                                        ValidatesOnExceptions=true}" />
                <Label Grid.Row="9"
                       Grid.Column="0"
                       Margin="3"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       Content="Message:"
                       Foreground="White" />
                <TextBox x:Name="messageTextBox"
                         Grid.Row="9"
                         Grid.Column="1"
                         Width="120"
                         Height="23"
                         Margin="3"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Center"
                         Background="Transparent"
                         Text="{Binding Message,
                                        Mode=TwoWay,
                                        NotifyOnValidationError=true,
                                        ValidatesOnExceptions=true}" />
                <Label Grid.Row="10"
                       Grid.Column="0"
                       Margin="3"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       Content="Number Decking:"
                       Foreground="White" />
                <TextBox x:Name="numberDeckingTextBox"
                         Grid.Row="10"
                         Grid.Column="1"
                         Width="120"
                         Height="23"
                         Margin="3"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Center"
                         Background="Transparent"
                         Text="{Binding NumberDecking,
                                        Mode=TwoWay,
                                        NotifyOnValidationError=true,
                                        ValidatesOnExceptions=true}" />
                <Label Grid.Row="11"
                       Grid.Column="0"
                       Margin="3"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       Content="Num Of Layers:"
                       Foreground="White" />
                <TextBox x:Name="numOfLayersTextBox"
                         Grid.Row="11"
                         Grid.Column="1"
                         Width="120"
                         Height="23"
                         Margin="3"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Center"
                         Background="Transparent"
                         Text="{Binding NumOfLayers,
                                        Mode=TwoWay,
                                        NotifyOnValidationError=true,
                                        ValidatesOnExceptions=true}" />
                <Label Grid.Row="12"
                       Grid.Column="0"
                       Margin="3"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       Content="Pitch:"
                       Foreground="White" />
                <TextBox x:Name="pitchTextBox"
                         Grid.Row="12"
                         Grid.Column="1"
                         Width="120"
                         Height="23"
                         Margin="3"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Center"
                         Background="Transparent"
                         Text="{Binding Pitch,
                                        Mode=TwoWay,
                                        NotifyOnValidationError=true,
                                        ValidatesOnExceptions=true}" />
                <Label Grid.Row="13"
                       Grid.Column="0"
                       Margin="3"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       Content="Plane ID:"
                       Foreground="White" />
                <TextBox x:Name="planeIDTextBox"
                         Grid.Row="13"
                         Grid.Column="1"
                         Width="120"
                         Height="23"
                         Margin="3"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Center"
                         Background="Transparent"
                         Text="{Binding PlaneID,
                                        Mode=TwoWay,
                                        NotifyOnValidationError=true,
                                        ValidatesOnExceptions=true}" />
                <Label Grid.Row="14"
                       Grid.Column="0"
                       Margin="3"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       Content="Plane Type ID:"
                       Foreground="White" />
                <TextBox x:Name="planeTypeIDTextBox"
                         Grid.Row="14"
                         Grid.Column="1"
                         Width="120"
                         Height="23"
                         Margin="3"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Center"
                         Background="Transparent"
                         Text="{Binding PlaneTypeID,
                                        Mode=TwoWay,
                                        NotifyOnValidationError=true,
                                        ValidatesOnExceptions=true}" />
                <Label Grid.Row="15"
                       Grid.Column="0"
                       Margin="3"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       Content="Rake Length:"
                       Foreground="White" />
                <TextBox x:Name="rakeLengthTextBox"
                         Grid.Row="15"
                         Grid.Column="1"
                         Width="120"
                         Height="23"
                         Margin="3"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Center"
                         Background="Transparent"
                         Text="{Binding RakeLength,
                                        Mode=TwoWay,
                                        NotifyOnValidationError=true,
                                        ValidatesOnExceptions=true}" />
                <Label Grid.Row="16"
                       Grid.Column="0"
                       Margin="3"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       Content="Ridge Length:"
                       Foreground="White" />
                <TextBox x:Name="ridgeLengthTextBox"
                         Grid.Row="16"
                         Grid.Column="1"
                         Width="120"
                         Height="23"
                         Margin="3"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Center"
                         Background="Transparent"
                         Text="{Binding RidgeLength,
                                        Mode=TwoWay,
                                        NotifyOnValidationError=true,
                                        ValidatesOnExceptions=true}" />
                <Label Grid.Row="17"
                       Grid.Column="0"
                       Margin="3"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       Content="Square Footage:"
                       Foreground="White" />
                <TextBox x:Name="squareFootageTextBox"
                         Grid.Row="17"
                         Grid.Column="1"
                         Width="120"
                         Height="23"
                         Margin="3"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Center"
                         Background="Transparent"
                         Text="{Binding SquareFootage,
                                        Mode=TwoWay,
                                        NotifyOnValidationError=true,
                                        ValidatesOnExceptions=true}" />
                <Label Grid.Row="18"
                       Grid.Column="0"
                       Margin="3"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       Content="Step Flashing:"
                       Foreground="White" />
                <TextBox x:Name="stepFlashingTextBox"
                         Grid.Row="18"
                         Grid.Column="1"
                         Width="120"
                         Height="23"
                         Margin="3"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Center"
                         Background="Transparent"
                         Text="{Binding StepFlashing,
                                        Mode=TwoWay,
                                        NotifyOnValidationError=true,
                                        ValidatesOnExceptions=true}" />
                <Label Grid.Row="19"
                       Grid.Column="0"
                       Margin="3"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       Content="Success Flag:"
                       Foreground="White"
                       Visibility="Collapsed" />
                <CheckBox x:Name="successFlagCheckBox"
                          Grid.Row="19"
                          Grid.Column="1"
                          Margin="3"
                          HorizontalAlignment="Left"
                          VerticalAlignment="Center"
                          Content=""
                          IsChecked="{Binding SuccessFlag,
                                              Mode=TwoWay,
                                              NotifyOnValidationError=true,
                                              ValidatesOnExceptions=true}"
                          Visibility="Collapsed" />
                <Label Grid.Row="20"
                       Grid.Column="0"
                       Margin="3"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       Content="Three And One:"
                       Foreground="White" />
                <TextBox x:Name="threeAndOneTextBox"
                         Grid.Row="20"
                         Grid.Column="1"
                         Width="120"
                         Height="24"
                         Margin="3"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Top"
                         Background="Transparent"
                         Text="{Binding ThreeAndOne,
                                        Mode=TwoWay,
                                        NotifyOnValidationError=true,
                                        ValidatesOnExceptions=true}" />
            </Grid>

            <StackPanel x:Name="stkpnl"
                        Width="258"
                        DockPanel.Dock="Right">
                <StackPanel HorizontalAlignment="Right"
                            Background="Transparent"
                            Orientation="Horizontal">
                    <TextBlock Width="150"
                               Background="Transparent"
                               Foreground="White"
                               Text="Predominant Pitch"
                               TextAlignment="Center" />
                    <TextBox x:Name="PrePitch"
                             Width="100"
                             Height="23"
                             HorizontalAlignment="Right"
                             Background="Transparent"
                             BorderBrush="{DynamicResource ReverseBackgroundGradient}"
                             CaretBrush="White"
                             Foreground="White"
                             TextWrapping="Wrap" />
                </StackPanel>
                <StackPanel HorizontalAlignment="Right"
                            Background="Transparent"
                            Orientation="Horizontal">
                    <TextBlock Width="150"
                               Background="Transparent"
                               Foreground="White"
                               Text="Ridges"
                               TextAlignment="Center" />
                    <TextBox x:Name="Ridges"
                             Width="100"
                             Height="23"
                             HorizontalAlignment="Right"
                             Background="Transparent"
                             BorderBrush="{DynamicResource ReverseBackgroundGradient}"
                             CaretBrush="White"
                             Foreground="White"
                             TextWrapping="Wrap" />
                </StackPanel>
                <StackPanel HorizontalAlignment="Right"
                            Background="Transparent"
                            Orientation="Horizontal">
                    <TextBlock Width="150"
                               Background="Transparent"
                               Foreground="White"
                               Text="Hips"
                               TextAlignment="Center" />
                    <TextBox x:Name="Hips"
                             Width="100"
                             Height="23"
                             HorizontalAlignment="Right"
                             Background="Transparent"
                             BorderBrush="{DynamicResource ReverseBackgroundGradient}"
                             CaretBrush="White"
                             Foreground="White"
                             TextWrapping="Wrap" />
                </StackPanel>
                <StackPanel HorizontalAlignment="Right"
                            Background="Transparent"
                            Orientation="Horizontal">
                    <TextBlock Width="150"
                               Background="Transparent"
                               Foreground="White"
                               Text="Valleys"
                               TextAlignment="Center" />
                    <TextBox x:Name="OrderIceWaterValleys"
                             Width="100"
                             Height="23"
                             HorizontalAlignment="Right"
                             Background="Transparent"
                             BorderBrush="{DynamicResource ReverseBackgroundGradient}"
                             CaretBrush="White"
                             Foreground="White"
                             TextWrapping="Wrap" />
                </StackPanel>
                <StackPanel HorizontalAlignment="Right"
                            Background="Transparent"
                            Orientation="Horizontal">
                    <TextBlock Width="150"
                               Background="Transparent"
                               Foreground="White"
                               Text="Rakes"
                               TextAlignment="Center" />
                    <TextBox x:Name="Rakes"
                             Width="100"
                             Height="23"
                             HorizontalAlignment="Right"
                             Background="Transparent"
                             BorderBrush="{DynamicResource ReverseBackgroundGradient}"
                             CaretBrush="White"
                             Foreground="White"
                             TextWrapping="Wrap" />
                </StackPanel>
                <StackPanel HorizontalAlignment="Right"
                            Background="Transparent"
                            Orientation="Horizontal">
                    <TextBlock Width="150"
                               Background="Transparent"
                               Foreground="White"
                               Text="Eaves"
                               TextAlignment="Center" />
                    <TextBox x:Name="Eaves"
                             Width="100"
                             Height="23"
                             HorizontalAlignment="Right"
                             Background="Transparent"
                             BorderBrush="{DynamicResource ReverseBackgroundGradient}"
                             CaretBrush="White"
                             Foreground="White"
                             TextWrapping="Wrap" />
                </StackPanel>
                <StackPanel HorizontalAlignment="Right"
                            Background="Transparent"
                            Orientation="Horizontal">
                    <TextBlock Width="150"
                               Background="Transparent"
                               Foreground="White"
                               Text="Total Area (OFF)"
                               TextAlignment="Center" />
                    <TextBox x:Name="TotalAreaOFF"
                             Width="100"
                             Height="23"
                             HorizontalAlignment="Right"
                             Background="Transparent"
                             BorderBrush="{DynamicResource ReverseBackgroundGradient}"
                             CaretBrush="White"
                             Foreground="White"
                             TextWrapping="Wrap" />
                </StackPanel>
                <StackPanel HorizontalAlignment="Right"
                            Background="Transparent"
                            Orientation="Horizontal" />
                <StackPanel Background="Transparent" Orientation="Horizontal" />
                <StackPanel HorizontalAlignment="Right"
                            Background="Transparent"
                            Orientation="Horizontal" />
                <StackPanel Background="Transparent" Orientation="Horizontal" />

                <StackPanel HorizontalAlignment="Right"
                            Background="Transparent"
                            Orientation="Horizontal" />
                <StackPanel Background="Transparent" Orientation="Horizontal" />

                <StackPanel HorizontalAlignment="Right"
                            Background="Transparent"
                            Orientation="Horizontal">
                    <TextBlock Width="150"
                               Background="Transparent"
                               Foreground="White"
                               Text="#Squares to Order"
                               TextAlignment="Center" />
                    <TextBox x:Name="OrderSqShingle"
                             Width="100"
                             Height="23"
                             HorizontalAlignment="Right"
                             Background="Transparent"
                             BorderBrush="{DynamicResource ReverseBackgroundGradient}"
                             CaretBrush="White"
                             Foreground="White"
                             TextChanged="OrderSqShingle_TextChanged"
                             TextWrapping="Wrap" />
                </StackPanel>
                <StackPanel HorizontalAlignment="Right"
                            Background="Transparent"
                            Orientation="Horizontal" />
                <StackPanel HorizontalAlignment="Right"
                            Background="Transparent"
                            Orientation="Horizontal" />
                <StackPanel HorizontalAlignment="Right"
                            Background="Transparent"
                            Orientation="Horizontal">
                    <TextBlock Width="150"
                               Background="Transparent"
                               Foreground="White"
                               Text="Rolls of Underlayment"
                               TextAlignment="Center" />
                    <TextBox x:Name="OrderUnderlayment"
                             Width="100"
                             Height="23"
                             HorizontalAlignment="Right"
                             Background="Transparent"
                             BorderBrush="{DynamicResource ReverseBackgroundGradient}"
                             CaretBrush="White"
                             Foreground="White"
                             TextWrapping="Wrap">
                        <TextBox.SelectionBrush>
                            <SolidColorBrush Color="#FF3399FF" />
                        </TextBox.SelectionBrush>
                    </TextBox>
                </StackPanel>
                <StackPanel HorizontalAlignment="Right"
                            Background="Transparent"
                            Orientation="Horizontal">
                    <TextBlock Width="150"
                               Background="Transparent"
                               Foreground="White"
                               Text="Hip/Ridge"
                               TextAlignment="Center" />
                    <TextBox x:Name="OrderHipandRidge"
                             Width="100"
                             Height="23"
                             HorizontalAlignment="Right"
                             Background="Transparent"
                             BorderBrush="{DynamicResource ReverseBackgroundGradient}"
                             CaretBrush="White"
                             Foreground="White"
                             TextWrapping="Wrap" />
                </StackPanel>
                <StackPanel HorizontalAlignment="Right"
                            Background="Transparent"
                            Orientation="Horizontal">
                    <TextBlock x:Name="textBlock1_Copy1"
                               Width="150"
                               Background="Transparent"
                               Foreground="White"
                               Text="Ice &amp; Water"
                               TextAlignment="Center" />
                    <TextBox x:Name="OrderIceWater"
                             Width="100"
                             Height="23"
                             HorizontalAlignment="Right"
                             Background="Transparent"
                             BorderBrush="{DynamicResource ReverseBackgroundGradient}"
                             CaretBrush="White"
                             Foreground="White"
                             TextWrapping="Wrap" />

                </StackPanel>
                <StackPanel HorizontalAlignment="Right"
                            Background="Transparent"
                            Orientation="Horizontal">
                    <TextBlock Width="150"
                               Background="Transparent"
                               Foreground="White"
                               Text="StarterStrip"
                               TextAlignment="Center" />
                    <TextBox x:Name="OrderStarter"
                             Width="100"
                             Height="23"
                             HorizontalAlignment="Right"
                             Background="Transparent"
                             BorderBrush="{DynamicResource ReverseBackgroundGradient}"
                             CaretBrush="White"
                             Foreground="White"
                             TextWrapping="Wrap" />
                </StackPanel>

                <StackPanel HorizontalAlignment="Right"
                            Background="Transparent"
                            Orientation="Horizontal">
                    <TextBlock Width="150"
                               Background="Transparent"
                               Foreground="White"
                               Text="Box/RoofNails"
                               TextAlignment="Center" />
                    <TextBox x:Name="OrderRoofNails"
                             Width="100"
                             Height="23"
                             HorizontalAlignment="Right"
                             Background="Transparent"
                             BorderBrush="{DynamicResource ReverseBackgroundGradient}"
                             CaretBrush="White"
                             Foreground="White"
                             TextWrapping="Wrap" />
                </StackPanel>
                <StackPanel HorizontalAlignment="Right"
                            Background="Transparent"
                            Orientation="Horizontal">
                    <TextBlock Width="150"
                               Background="Transparent"
                               Foreground="White"
                               Text="Buckets/Button Caps"
                               TextAlignment="Center" />
                    <TextBox x:Name="OrderButtonCaps"
                             Width="100"
                             Height="23"
                             HorizontalAlignment="Right"
                             Background="Transparent"
                             BorderBrush="{DynamicResource ReverseBackgroundGradient}"
                             CaretBrush="White"
                             Foreground="White"
                             TextWrapping="Wrap" />
                </StackPanel>

                <StackPanel HorizontalAlignment="Right"
                            Background="Transparent"
                            Orientation="Horizontal">
                    <TextBlock Width="150"
                               Background="Transparent"
                               Foreground="White"
                               Text="Drip Edge"
                               TextAlignment="Center" />
                    <TextBox x:Name="OrderDripEdge"
                             Width="100"
                             Height="23"
                             HorizontalAlignment="Right"
                             Background="Transparent"
                             BorderBrush="{DynamicResource ReverseBackgroundGradient}"
                             CaretBrush="White"
                             Foreground="White"
                             TextWrapping="Wrap" />
                </StackPanel>

                <StackPanel HorizontalAlignment="Right"
                            Background="Transparent"
                            Orientation="Horizontal">
                    <TextBlock Width="150"
                               Background="Transparent"
                               Foreground="White"
                               Text="RidgeVent"
                               TextAlignment="Center" />
                    <TextBox x:Name="OrderRidgeVent"
                             Width="100"
                             Height="23"
